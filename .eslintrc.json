{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "rules": {
        "no-console": "off",
        "no-underscore-dangle":"off",
        "linebreak-style": "off",
        "camelcase": "off",
        "eslint-plugin-import/no-extraneous-dependencies": "off",
        "no-unused-vars": "off"
    }
}
